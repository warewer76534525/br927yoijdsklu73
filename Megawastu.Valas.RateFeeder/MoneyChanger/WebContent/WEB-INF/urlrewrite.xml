<?xml version="1.0" encoding="utf-8"?>
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/</from>
		<to>/moneychanger/welcome</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/moneychanger/$1</to>
	</rule>
	<outbound-rule>
		<from>/moneychanger/**</from>
		<to>/$1</to>
	</outbound-rule>	
</urlrewrite>
